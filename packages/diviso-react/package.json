{
  "author": "planttheidea",
  "bugs": {
    "url": "https://github.com/planttheidea/diviso/issues"
  },
  "dependencies": {
    "@types/use-sync-external-store": "^0.0.3",
    "diviso": "*",
    "diviso-shared": "*",
    "use-sync-external-store": "^1.2.0"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./esm/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "types": "./index.d.ts",
      "module": "./esm/index.js",
      "default": "./index.js"
    }
  },
  "homepage": "https://github.com/planttheidea/diviso#readme",
  "description": "React utilities for diviso state manager.",
  "keywords": [
    "react",
    "state"
  ],
  "license": "MIT",
  "main": "./index.js",
  "name": "diviso-react",
  "repository": {
    "directory": "packages/diviso-react",
    "type": "git",
    "url": "git+https://github.com/planttheidea/diviso.git"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c ../../rollup.config.js --config-react",
    "clean": "shx rm -rf dist",
    "copy": "shx cp package.json dist && shx cp -r dist/src dist/esm && shx cp -r dist/*.d.ts dist/esm",
    "dev": "NODE_ENV=development webpack-dev-server --config=examples/webpack.config.js",
    "dist": "npm run clean && npm run build && npm run copy && npm run patch:esm-ts",
    "lint": "eslint index.ts src",
    "patch:esm-ts": "node -e \"require('shelljs').find('dist/esm/**/*.d.ts').forEach(f=>require('fs').copyFileSync(f,f.replace(/\\.ts$/,'.mts')))\"; shx sed -i \"s/ from '(\\..*)';$/ from '\\$1.mjs';/\" 'dist/esm/**/*.d.mts'; shx sed -i \"s/^declare module '(\\..*)'/declare module '\\$1.mjs'/\" 'dist/esm/**/*.d.mts'"
  },
  "type": "module",
  "types": "./index.d.ts",
  "version": "0.1.0",
  "devDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@react-spring/web": "^9.6.1",
    "html-react-parser": "^3.0.4",
    "react-error-boundary": "^3.1.4",
    "redux-saga": "^1.2.2"
  }
}
