{
  "author": "planttheidea",
  "bugs": {
    "url": "https://github.com/planttheidea/diviso/issues"
  },
  "dependencies": {
    "redux-thunk": "^2.4.2"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./esm/core.d.mts",
        "default": "./esm/core.mjs"
      },
      "types": "./core.d.ts",
      "module": "./esm/core.js",
      "default": "./core.js"
    },
    "./*": {
      "import": {
        "types": "./esm/*.d.mts",
        "default": "./esm/*.mjs"
      },
      "types": "./*.d.ts",
      "module": "./esm/*.js",
      "default": "./*.js"
    }
  },
  "homepage": "https://github.com/planttheidea/diviso#readme",
  "description": "Think local, act global. State management for modern JS apps.",
  "keywords": [
    "state"
  ],
  "license": "MIT",
  "main": "./core.js",
  "name": "diviso",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/diviso.git"
  },
  "type": "module",
  "types": "./code.d.ts",
  "version": "1.0.0",
  "private": false
}
